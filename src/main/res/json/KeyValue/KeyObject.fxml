<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<HBox

        styleClass="bordered" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.github.insanusmokrassar.AutoPostBotConfigBuilder.controllers.json.KeyValueWrappers.KeyObjectController">

    <Button text="-" onAction="#remove">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </HBox.margin>
    </Button>
    <TextField promptText="Key" fx:id="key">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </HBox.margin>
    </TextField>
    <fx:include source="../ValueObject.fxml" fx:id="value"/>
    <padding>
        <Insets right="10.0"/>
    </padding>
</HBox>
