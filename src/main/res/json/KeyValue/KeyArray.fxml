<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<HBox

        styleClass="bordered"
        alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.github.insanusmokrassar.AutoPostBotConfigBuilder.controllers.json.KeyValueWrappers.KeyArrayController">

    <Button onAction="#remove" text="-">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </HBox.margin>
    </Button>
    <TextField fx:id="key" promptText="Key">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </HBox.margin>
    </TextField>
    <fx:include fx:id="value" source="../ValueArray.fxml"/>
    <padding>
        <Insets right="10.0"/>
    </padding>

</HBox>
